# -*- coding: UTF-8 -*-
import time;

myTime=time.time()
print(myTime)


#获取当前时间
localTime=time.localtime(myTime)
print(localTime)


#获取格式化后的时间

timer = time.asctime(localTime)
print(timer)

print(time.strftime("%H:%M:%S"))
print(time.strftime("%Y-%m-%d %H:%M:%S"))

