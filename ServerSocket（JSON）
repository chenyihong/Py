while True:
    client, addr = s.accept()
    # print(client)
    print(addr)
    # client.send(context)
    client.send(str(len(context)))
    client.send(context)
    request = client.recv(10240)
    print(request)
    print("-------------------------------")
    if request == "close":
        s.close()
        print("ServerSocket is closed...")
        break
