# -*- coding: UTF-8 -*-
import socket;

s = socket.socket()

host = socket.gethostname()
print(host)

hostInfo = socket.gethostbyaddr(host)  # 获取用户名称，MAC地址
print(hostInfo)

myIPAdd = socket.gethostbyname(host)  # 获取网络地址
print(myIPAdd)

port = 1314

s.bind((host, port))

s.listen(5)
print("-------------------------------")
